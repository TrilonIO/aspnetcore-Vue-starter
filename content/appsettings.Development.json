{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "WebRoot": "null",
  "Port": 5000,
  "LiveReload": {
    "LiveReloadEnabled": true,
